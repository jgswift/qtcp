{
    "name": "jgswift/qtcp",
    "description": "Web socket server/client using php and javascript",
    "keywords": ["php", "websockets", "server", "tcp"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Josh Swift",
            "email": "joshuaswift@gmail.com",
            "homepage": "http://www.kinesisphp.com",
            "role": "Developer"
        }
    ],    
    "require": {
        "php": ">=5.5",
        "cboden/ratchet": "0.3.*",
        "symfony/console": "2.5.*",
        "jgswift/qtil": "0.1.*",
        "jgswift/qio": "0.1.*",
        "jgswift/observr": "0.1.*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "danvk/dygraphs",
                "version": "v1.0.1",
                "dist": {
                    "url": "http://github.com/danvk/dygraphs/archive/v1.0.1.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "http://github.com/danvk/dygraphs",
                    "type": "git",
                    "reference": "release-1.0.1"
                }
            }
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "danvk/dygraphs": "v1.0.1"
    },
    "autoload": {
        "psr-4": {
            "qtcp\\": "src/",
            "qtcp\\Tests\\": "tests/"
        }
    }
}